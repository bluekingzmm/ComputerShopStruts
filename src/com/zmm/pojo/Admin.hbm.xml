<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.zmm.pojo.Admin" table="admin">
        <id name="id" type="java.lang.String" column="id">
            <generator class="uuid.hex"/>
        </id>
        <property name="admin_name" type="java.lang.String" column="admin_name"/>
        <property name="admin_password" type="java.lang.String" column="admin_password"/>
        <property name="status" type="java.lang.Integer" column="status"/>
    </class>
</hibernate-mapping>