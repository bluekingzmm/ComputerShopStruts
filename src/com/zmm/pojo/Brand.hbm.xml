<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.zmm.pojo.Brand" table="brand">
        <id name="brand_id" type="java.lang.Integer" column="brand_id">
            <generator class="native"/>
        </id>
        <property name="brand_name" type="java.lang.String" column="brand_name"/>
       
    </class>
</hibernate-mapping>