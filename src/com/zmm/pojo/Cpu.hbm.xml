<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.zmm.pojo.Cpu" table="cpu">
        <id name="cpu_id" type="java.lang.Integer" column="cpu_id">
            <generator class="native"/>
        </id>
        <property name="cpu_name" type="java.lang.String" column="cpu_name"/>
    </class>
</hibernate-mapping>